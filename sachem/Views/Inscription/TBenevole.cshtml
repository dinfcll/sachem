@model sachem.Models.Cours
@{
    ViewBag.Title = "Inscription au service Sachem - Tuteur de bénévole et rémunéré";
}
<div style="margin-right:15%">
    <h2>@ViewBag.Title</h2>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <h3 style="background-color:aliceblue">Disponibilité</h3>
        <div class="row rowCourseReussisPadTop">
            <div class="col-md-8"><h5 class="hIns">Combien d'heures êtes vous pret à donner à chaque semaine (1h30/élève aidé)</h5></div>
            <div class="col-md-4">
                <select style="width:300px;float:right;" class="form-search ins-select"></select>
            </div>
        </div>

        @Html.Partial("_CoursReussis")
    }
</div>
<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
