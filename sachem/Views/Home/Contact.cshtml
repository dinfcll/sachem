@model sachem.Models.p_Contact
@{
    ViewBag.Title = "Nous contacter";
}
<h2>@ViewBag.Title.</h2>       

    <div class="form-horizontal">

        <div class="form-group">
             <b>Personne à contacter : </b> @Html.DisplayFor(model => model.Prenom) @Html.DisplayFor(model => model.Nom)
        </div>

        <div class="form-group">
             <b>Numéro de téléphone : </b>@Html.DisplayFor(model => model.Telephone) #@Html.DisplayFor(model => model.Poste)
        </div>
        <div class="form-group">
            <b>Local : </b>@Html.DisplayFor(model => model.Local)
        </div>
        <div class="form-group">
             @*<b>Page Facebook : </b><a href=@Model.Facebook target="_blank">@Html.DisplayFor(model => model.Facebook)</a>*@
               <img src="~/Images/facebook.png" alt="Page facebook du SACHEM "width="50" height="50" /> <a href=@Model.Facebook target="_blank"></a>
            @*<b>Site web : </b><a href=@Model.SiteWeb target="_blank">@Html.DisplayFor(model => model.SiteWeb)</a>*@
               <img src="~/Images/www.png" alt="Site web du service d'aide" width="50" height="50" /> <a href=@Model.SiteWeb target="_blank"></a>
        </div>
        
    </div>

