@model sachem.Models.Personne

@{
    ViewBag.Title = "Ajouter enseignant";
}

<h2>@ViewBag.Title</h2>


@using (Html.BeginForm("Create", "Enseignant", FormMethod.Post)) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        @Html.Partial("_Enseignant")
    </div>
}

<div>
    @Html.ActionLink("Retour à la recherche des enseignants", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
