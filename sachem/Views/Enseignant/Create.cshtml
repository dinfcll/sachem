@using sachem.Models
@model Personne

@{
    ViewBag.Title = Messages.EnseignantAjouterTitre;
}

<h2>@ViewBag.Title</h2>

@using (Html.BeginForm("Create", "Enseignant", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        @Html.Partial("_Enseignant")
    </div>
}
