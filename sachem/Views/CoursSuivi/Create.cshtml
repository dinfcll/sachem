@model sachem.Models.CoursSuivi

@{
    ViewBag.Title = "Ajouter un cours de mathématiques antérieur";
}

<h2>Ajouter un cours de mathématiques antérieur</h2>


@using (Html.BeginForm()) 
{
    @Html.AntiForgeryToken()
    
    <div class="form-horizontal">
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        @Html.Partial("_CoursSuivi")
    </div>
}

<div>
    @Html.ActionLink("Retour au dossier de l'étudiant", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
