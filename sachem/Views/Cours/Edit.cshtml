@model sachem.Models.Cours

@{
    ViewBag.Title = "Modifier cours";
}

<h2>@ViewBag.Title</h2>


@using (Html.BeginForm("Edit", "Cours", FormMethod.Post))
{

    @Html.AntiForgeryToken()

    <div class="form-horizontal">

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.id_Cours)


        @Html.Partial("_Cours")

    </div>
}


@Html.Partial("_Footer")